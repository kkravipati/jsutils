{
  "name": "@modil-io/jsutils",
  "version": "1.0.0",
  "description": "jsutils library",
  "main": "dist/@modil-io/jsutils.js",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "docs:build": "documentation build src/** -f html -o docs"
  },
  "keywords": [
    "javascript",
    "utils",
    "javascript utilities",
    "library"
  ],
  "author": "",
  "license": "Apache",
  "bugs": {
    "url": ""
  },
  "homepage": "https://github.com/modil-io/jsutils",
  "dependencies": {
    "es6-promise": "^4.2.5",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.0.1",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.uniq": "^4.5.0",
    "omit-deep-lodash": "^1.1.4",
    "selectn": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-object-assign": "7.0.0-beta.4",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "documentation": "^11.0.1",
    "es6-promise": "^4.2.5",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-jsdoc": "^8.0.1",
    "fs-extra": "^6.0.1",
    "jsdoc": "^3.6.2",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^13.1.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "plugins": [
    "jsdoc"
  ],
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
